{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "no-console": "warn",
    "eqeqeq": "error",
    "quotes": [2, "double", "backtick"]
  }
}
